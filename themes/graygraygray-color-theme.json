{
  "name": "dark-graygraygray",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#202020",
    "editor.foreground": "#EFEFEF",
    "editor.lineHighlightBackground": "#303030",
    "editorGutter.background": "#282828",
    "editorWhitespace.foreground": "#CECECE",

    "editor.foldBackground": "#40C080",
    "editor.selectionBackground": "#007777",
    "editor.selectionHighlightBackground": "#606060",

    "editorHoverWidget.background": "#202020",
    "editorHoverWidget.foreground": "#EFEFEF",
    "editorHoverWidget.border": "#EFEFEF",

    "editorIndentGuide.background": "#808080",
    "editorOverviewRuler.bracketMatchForeground": "#40C080",

    "editorSuggestWidget.background": "#2C2C2C",
    "editorSuggestWidget.foreground": "#EFEFEF",
    "editorSuggestWidget.selectedBackground": "#3B82F6",

    "editorCursor.foreground": "#EFEFEF",

    "editorGroupHeader.tabsBackground": "#2C2C2C",
    "editorGroupHeader.noTabsBackground": "#2C2C2C",
    "tab.inactiveBackground": "#2C2C2C",
    "tab.activeBackground": "#2C2C2C",
    "tab.border": "#2C2C2C",

    "editorBracketHighlight.foreground1": "#00C0C0",
    "editorBracketHighlight.foreground2": "#CFA07A",
    "editorBracketHighlight.foreground3": "#87CEEB",
    "editorBracketHighlight.foreground4": "#CFC0CB",
    "editorBracketHighlight.foreground5": "#98CB98",
    "editorBracketHighlight.foreground6": "#DDA0DD",
    "editorBracketMatch.border": "#202020",
    "editorBracketMatch.background": "#E420EF",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000",

    "breadcrumb.background": "#202020",
    "editorLineNumber.activeForeground": "#00FFFF",

    "titleBar.activeBackground": "#202020",
    "titleBar.activeForeground": "#EFEFEF",
    "titleBar.inactiveBackground": "#2C2C2C",

    "menu.background": "#202020",

    "icon.foreground": "#EFEFEF",

    "searchEditor.textInputBorder": "#EFEFEF",

    "badge.background": "#003333",
    "badge.foreground": "#EFEFEF",
    "breadcrumb.focusForeground": "#3B82F6",
    "breadcrumb.foreground": "#EFEFEF",
    "list.hoverBackground": "#404040",
    "list.inactiveSelectionBackground": "#404040",
    "list.activeSelectionBackground": "#3B82F6",
    "list.filterMatchBorder": "#EFEFEF",
    "minimap.background": "#28282899",
    "menu.foreground": "#EFEFEF",
    "sideBarSectionHeader.background": "#4D4D4D",
    "sideBar.background": "#202020",
    "sideBar.foreground": "#EFEFEF",
    "tab.activeForeground": "#EFEFEF",
    "tab.unfocusedActiveBackground": "#404040",
    "tree.indentGuidesStroke": "#EFEFEF",
    "scrollbar.shadow": "#ff000000",

    "editor.findMatchBackground": "#FFFF00",
    "editor.findMatchHighlightBackground": "#A0A030",
    "editor.findRangeHighlightBackground": "#606060",
    "editorOverviewRuler.findMatchForeground": "#FFFF00",
    "minimap.findMatchHighlight": "#FFFF00",

    "contrastActiveBorder": "#3B82F6",
    "contrastBorder": "#EFEFEF",
    "editorGroup.border": "#EFEFEF",
    "focusBorder": "#3B82F6",
    "panel.border": "#EFEFEF",
    "titleBar.border": "#EFEFEF",
    "menu.border": "#EFEFEF",

    "terminal.ansiBlack": "#1E1E1E",
    "terminal.ansiBrightBlack": "#5D5D5D",
    "terminal.ansiWhite": "#EFEFEF",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiRed": "#FF5252",
    "terminal.ansiBrightRed": "#FF8383",
    "terminal.ansiGreen": "#69F0AE",
    "terminal.ansiBrightGreen": "#B9F6CA",
    "terminal.ansiBlue": "#82AAFF",
    "terminal.ansiBrightBlue": "#B0CFFF",
    "terminal.ansiCyan": "#00B0FF",
    "terminal.ansiBrightCyan": "#80D4FF",
    "terminal.ansiMagenta": "#B388FF",
    "terminal.ansiBrightMagenta": "#D1A5FF",
    "terminal.ansiYellow": "#FFFF00",
    "terminal.ansiBrightYellow": "#FFFF6B"
  },

  "tokenColors": [
    // Reference from https://www.sublimetext.com/docs/3/scope_naming.html
    {
      "scope": [
        "entity.name",
        "entity.other.inherited-class",
        "storage.type.built-in"
      ],
      "settings": {
        "foreground": "#78DCE8"
      }
    },
    {
      "scope": ["storage.type.built-in"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.label",
        "entity.name.section",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#F25C54",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.function.operator" // +=,-= ...
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter",
        "variable.function",
        "variable.language"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "scope": [
        "variable.language" // this, self, super, etc.
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "keyword.other.unit" // 0x
      ],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.language", // booleans, nullptr
        "constant.character.escape"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#88EEFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "storage.modifier",
        "storage.control",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#00ACCC",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": ["support"],
      "settings": {
        "foreground": "#F1F1F1"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.bitwise",
        "keyword.operator.logical",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.noexcept",
        "keyword.operator.wordlike",
        "keyword.operator.word",
        "keyword.declaration",
        "markup.heading", // #
        "punctuation.definition.markdown" //
      ],
      "settings": {
        "foreground": "#00B0B0",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.bitwise",
        "keyword.operator.logical"
      ],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "punctuation.definition.bracket",
        "punctuation.definition.begin.bracket.square",
        "punctuation.definition.end.bracket.square",
        "punctuation.section.angle-brackets",
        "punctuation.section.block.begin.bracket.curly",
        "punctuation.section.block.end.bracket.curly",
        "punctuation.section.parens.begin.bracket.round",
        "punctuation.section.parens.end.bracket.round",
        "punctuation.section.parameters.begin.bracket.round",
        "punctuation.section.parameters.end.bracket.round",
        "punctuation.section.arguments.begin.bracket.round",
        "punctuation.section.arguments.end.bracket.round",
        "storage.modifier.array.bracket.square",
        "keyword.operator.delete.array.bracket",
        "keyword.operator.comparison",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#00FFFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.separator",
        "punctuation.separator.dictionary.key-value",
        "punctuation.separator.hash",
        "punctuation.separator.inheritance",
        "punctuation.separator.key-value",
        "punctuation.separator.namespace",
        "punctuation.separator.pointer-access",
        "punctuation.separator.slice",
        "punctuation.separator.annotation",
        "punctuation.separator.delimiter"
      ],
      "settings": {
        "foreground": "#EFEFEF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "fenced_code.block.language",
        "markup.fenced_code.block", // ```*```
        "markup.inline.raw" // `*`
      ],
      "settings": {
        "foreground": "#66DFFF"
      }
    },
    {
      "scope": [
        "comment",
        "markup.quote" // >
      ],
      "settings": {
        "foreground": "#009900",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#D000D0"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.underline.link",
        "meta.link",
        "punctuation.definition.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#00FFFF",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#F1F1F1"
      }
    }
  ]
}
